<root>
	<styles>
		<include src="file://{resources}/layout/custom_game/common/override_link/override_link.css" />
	</styles>

	<scripts>
		<include src="file://{resources}/layout/custom_game/common/override_link/override_link.js" />
	</scripts>

	<Panel class="OverrideRoot">
		<Panel id="OverrideWindow">
			<Panel id="OverrideTextWrap">
				<Label id="OverrideText" text="#new_link"/>
			</Panel>
			<Image
				id="PitureLink"
				onactivate="ExternalBrowserGoToURL('https://steamcommunity.com/sharedfiles/filedetails/?id=2141084572')"
				src="file://{resources}/images/custom_game/overthrow.png"
			/>
			<Panel id="OverrideTipWrap">
				<Label id="OverrideTipText" text="#new_link_tip"/>
			</Panel>
		</Panel>
	</Panel>
</root>
